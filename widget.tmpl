<script id="widget-template" type="mustache/x-tmpl-mustache">
  <li id="<# widget.name #>">
    <a href="#" class="verticals-list_link">
    <span class="fa <# widget.icon #>"></span>
    <div class="text-wrapper">
      <h2>
        <# widget.name #>
      </h2>
      <h3>
        <span><# widget.cta #></span>
        <svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
          <path d="M22 121-20 12 5-12-5-12z"></path>
        </svg>
      </h3>
    </div>
    <span class="sr-only">Opens in new window</span>
  </a>
  </li>
</script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/mustache.js/2.3.0/mustache.min.js"
  charset="utf-8"></script>

<script type="text/javascript">
  var widget = {
    name: "Phone",
    icon: "fa-phone",
    cta: "Call Us"
  }
  var template = $('#template').html();
  var parseTags = new Array();
  parseTags.push("<#");
  parseTags.push("#>");
  Mustache.parse(template, parseTags);
  var rendered = Mustache.render(template, {
    name: "Al"
  });
  $('#target').html(rendered);
</script>